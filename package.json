{
  "name": "vabhub-frontend",
  "version": "1.6.0",
  "description": "VabHub 现代化多仓库架构前端界面",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . -c .eslintrc.js --fix --ext .ts,.js,.vue,.tsx,.jsx",
    "format": "prettier --write src/",
    "test": "vitest",
    "build:icons": "tsc -b src/@iconify && node src/@iconify/build-icons.js",
    "postinstall": "npm run build:icons",
    "pkg": "pkg . -t node18-win-x64 -o VabHub-Frontend.exe"
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-i18n": "^9.10.1",
    "axios": "^1.7.9",
    "lodash-es": "^4.17.21",
    "element-plus": "^2.4.0",
    "@element-plus/icons-vue": "^2.3.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "@vueuse/core": "^12.4.0",
    "@vueuse/math": "^12.4.0",
    "dayjs": "^1.11.13",
    "nprogress": "^0.2.0",
    "js-cookie": "^3.0.5",
    "crypto-js": "^4.2.0",
    "qrcode.vue": "^3.6.0",
    "apexcharts": "^4.0.0",
    "vue3-apexcharts": "^1.8.0",
    "vuetify": "^3.6.0",
    "@iconify/vue": "^4.3.0",
    "@iconify-json/mdi": "^1.1.52",
    "colorthief": "^2.6.0",
    "copy-to-clipboard": "^3.3.3",
    "mousetrap": "^1.6.5",
    "body-scroll-lock": "^3.1.5",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuedraggable": "^4.1.0",
    "vue3-ace-editor": "^2.2.4",
    "ace-builds": "^1.37.4",
    "vue-toastification": "^2.0.0-rc.5",
    "webfontloader": "^1.6.28",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.0.10",
    "eslint": "^9.18.0",
    "prettier": "^3.0.3",
    "vite": "^5.4.11",
    "vitest": "^0.34.0",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "@iconify/tools": "^4.0.4",
    "@types/node": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/crypto-js": "^4.2.2",
    "@types/mousetrap": "^1.6.15",
    "@types/nprogress": "^0.2.3",
    "@types/webfontloader": "^1.6.34",
    "@types/lodash-es": "^4.17.12",
    "@types/body-scroll-lock": "^3.1.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vite-plugin-pages": "^0.32.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-top-level-await": "^1.5.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.12.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.13.2",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-use-logical-spec": "5.0.1",
    "postcss-html": "^1.5.0",
    "type-fest": "^4.15.0",
    "terser": "^5.36.0"
  },
  "keywords": [
    "vabhub",
    "frontend",
    "vue3",
    "typescript",
    "vite",
    "pwa",
    "music",
    "dashboard",
    "media-management"
  ],
  "author": "VabHub Team",
  "license": "MIT"
}